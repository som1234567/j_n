    "class Stack:\n",
    "    def __init__(self):\n",
    "        self._storage = []\n",
    "        \n",
    "    def __len__(self):\n",
    "        return len(self._storage)\n",
    "    \n",
    "    def push(self, item):\n",
    "        self._storage.append(item)\n",
    "    \n",
    "    def pop(self):\n",
    "        return self._storage.pop()\n"
